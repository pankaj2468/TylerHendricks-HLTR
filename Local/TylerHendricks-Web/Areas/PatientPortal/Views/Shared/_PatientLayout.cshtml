<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="googlebot" content="noindex">
    <title>@ViewData["Title"] - Hendrx Health</title>
    <link href="https://fonts.googleapis.com/css2?family=Oswald:wght@200;300;400;500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.22/css/jquery.dataTables.min.css">
    <link href="~/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/twitter-bootstrap/css/style.css?v=1.57" rel="stylesheet" />
    <link href="~/twitter-bootstrap/css/responsive.css?v=1.0.0.1.1" rel="stylesheet" />
    <link href="~/datatables/dataTables.bootstrap4.css" rel="stylesheet" />
    <link href="~/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
    @RenderSection("Styles", required: false)
    <script src="~/jquery/jquery.js"></script>
</head>
<body style="background: #fff;">
    <partial name="_Loader" />
    <div class="container-fluid QnAPage">
        <div class="row">
            @RenderBody()
        </div>
    </div>
    <!--Ajax Calling Urls start-->
    <script>
        var PatientPortalUrl = {
            PatientLoginUrl: '@Url.Action("Index", "Patient", new { area = "PatientPortal" })',
            TermsAndConditionUrl: '@Url.Action("GetTermsAndPolicyContent", "Home", new { area = "PatientPortal" })',
            MedicationGetDropdownUrl: '@Url.Action("GetMedicationDropdown", "Patient", new { area = "PatientPortal" })',
            MedicineUploadUrl: '@Url.Action("MedicineUpload", "Patient", new { area = "PatientPortal" })',
            PhotoIdUploadUrl: '@Url.Action("PhotoIdUpload", "Patient", new { area = "PatientPortal" })',
            OrderHistoryUrl: '@Url.Action("OrderHistory", "Patient", new { area = "PatientPortal" })',
            GetMedicationCategoryUrl: '@Url.Action("GetMedicationCategory", "Patient", new { area = "PatientPortal" })',
            PatientProfileUrl: '@Url.Action("Profile", "Patient", new { area = "PatientPortal" })',
            MedicineDosePostUrl: '@Url.Action("MedicineDose", "Patient", new { area = "PatientPortal" })',
            PatientMessageUrl: '@Url.Action("SaveChat", "Patient", new { area = "PatientPortal" })',
            PatientRetakeFileUrl: '@Url.Action("UploadFile", "Patient", new { area = "PatientPortal" })',
            GetMedicationUrl: '@Url.Action("GetMedication", "Patient", new { area = "PatientPortal" })',
            MessagesPageUrl: '@Url.Action("Messages", "Patient", new { area = "PatientPortal" })',
            LoginUrl: '@Url.Action("Login", "Home", new { area = "PatientPortal" })',
            MessageUploadUrl: '@Url.Action("MessageUploadFile", "Patient", new { area = "PatientPortal" })',
            SetSesstionUrl: '@Url.Action("SetSession", "Home", new { area = "PatientPortal" })',
            BindMessagesUrl: '@Url.Action("BindMessage", "Patient", new { area = "PatientPortal" })',
        };
    </script>
    <script src="~/jquery/popper.min.js"></script>
    <script src="~/jquery-validate/jquery.validate.js"></script>
    <script src="~/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.js"></script>
    <script src="~/twitter-bootstrap-JS/bootstrap.min.js"></script>
    <script src="~/datatables/jquery.dataTables.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="~/JS/QuestionNaire.js?v=1.55"></script>
    <script>
        $(function () {
            MenuTabSelect();
        });
        function MenuTabSelect() {
            let url = window.location.pathname;
            if (url.toLowerCase().indexOf('profile') != -1) {
                $('#headerdiv').children().removeClass('sky-btn').addClass('blue2-btn');
                $('#headerdiv').find('a:nth-child(3)').removeClass('blue2-btn').addClass('sky-btn');
            } else if (url.toLowerCase().indexOf('messages') != -1) {
                $('#headerdiv').children().removeClass('sky-btn').addClass('blue2-btn');
                $('#headerdiv').find('a:nth-child(2)').removeClass('blue2-btn').addClass('sky-btn');
            } else if (url.toLowerCase().indexOf('orderhistory') != -1) {
                $('#headerdiv').children().removeClass('sky-btn').addClass('blue2-btn');
                $('#headerdiv').find('a:nth-child(1)').removeClass('blue2-btn').addClass('sky-btn');
            }
        }
    </script>
    <!--Ajax Calling Urls end-->
    @RenderSection("Scripts", required: false)
</body>
</html>
