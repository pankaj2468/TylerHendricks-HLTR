<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="googlebot" content="noindex">
    <title>@ViewData["Title"] - Hendrx Health</title>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Oswald:wght@200;300;400;500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.25/css/jquery.dataTables.min.css" />
    <link href="~/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/twitter-bootstrap/css/responsive.css" rel="stylesheet" />
    <link href="~/twitter-bootstrap/css/swiper-bundle.min.css" rel="stylesheet" />
    <link href="~/Physician/style.css?v=1.0.0.8" rel="stylesheet" />
    <link href="~/datatables/dataTables.bootstrap4.css" rel="stylesheet" />
    <link href="~/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
</head>
<body>
    <partial name="_Loader" />
    <div class="container-fluid QnAPage">
        <div class="row">
            @RenderBody()
        </div>
    </div>
    <script src="~/jquery/jquery.js"></script>
    <script src="~/jquery-validate/jquery.validate.js"></script>
    <script src="~/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.js"></script>
    <script src="~/twitter-bootstrap-JS/bootstrap.min.js"></script>
    <!-- Custom scripts for all pages-->
    <script src="~/datatables/jquery.dataTables.min.js"></script>
    <script src="~/jquery/swiper-bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <!--Ajax Calling Urls start-->
    <script>
        var PhysicianPortalUrl = {
            PhysicianLoginUrl: '@Url.Action("Index", "Home", new { area = "PhysicianPortal" })',
            PhysicianDashboardUrl: '@Url.Action("PhysicianDashboard", "Home", new { area = "PhysicianPortal" })',
            PhysicianDashbordRecord: '@Url.Action("GetPatientRecords", "Home", new { area = "PhysicianPortal" })',
            PhysicianPatientChart: '@Url.Action("PatientChart", "Home", new { area = "PhysicianPortal" })',
            GetNotifyRecordsUrl: '@Url.Action("GetNotifyStates", "Home", new { area = "PhysicianPortal" })',
            ChangeAccountStatusUrl: '@Url.Action("ChanageAccountStatus", "Home", new { area = "PhysicianPortal" })',
            GetPatientChartUrl: '@Url.Action("GetPatientChart", "Home", new { area = "PhysicianPortal" })',
            RetakeRequestUrl: '@Url.Action("RetakeRequests", "Home", new { area = "PhysicianPortal" })',
            UpdateOrderStatusUrl: '@Url.Action("ChangeOrderStatus", "Home", new { area = "PhysicianPortal" })',
            UploadNoteUrl: '@Url.Action("UploadNote", "Home", new { area = "PhysicianPortal" })',
            ChangePatientOrderStatusUrl: '@Url.Action("changePatientOrderStatus", "Home", new { area = "PhysicianPortal" })',
            SaveNoteUrl: '@Url.Action("SaveNotesText", "Home", new { area = "PhysicianPortal" })',
            DeleteNoteUrl: '@Url.Action("deleteNote", "Home", new { area = "PhysicianPortal" })',
            PopulatePatientDataUrl: '@Url.Action("PopulatePatientData", "Home", new { area = "PhysicianPortal" })',
            PopulatePharmacyDataUrl: '@Url.Action("PopulatePharmacyData", "Home", new { area = "PhysicianPortal" })',
            PopulateMedicineDataUrl: '@Url.Action("PopulateMedicineData", "Home", new { area = "PhysicianPortal" })',
            UpdateMedicineUrl: '@Url.Action("UpdateMedicine", "Home", new { area = "PhysicianPortal" })',
            GetPatientChartByRowNoUrl: '@Url.Action("GetPatientChartByRowNo", "Home", new { area = "PhysicianPortal" })',
            ManualEDUrl: '@Url.Action("ED", "Home", new { area = "PhysicianPortal" })',
            ManualHairLossUrl: '@Url.Action("Hairloss", "Home", new { area = "PhysicianPortal" })',
            ManualMedicalRefillUrl: '@Url.Action("MR", "Home", new { area = "PhysicianPortal" })',
            MoveToAllUrl: '@Url.Action("RecordMoveToAllPatient", "Home", new { area = "PhysicianPortal" })'
        };
    </script>
    <!--Ajax Calling Urls end-->
    @RenderSection("Scripts", required: false)
    <script src="~/JS/PhysicianManual.js"></script>
</body>
</html>
